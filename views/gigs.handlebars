<section id="gigs" class="container">
    <h1>All Gigs</h1>
    {{#each gigs}}
    <div class="gig">
        <h3>{{this.dataValues.title}}</h3>
        <p>{{this.dataValues.description}}</p>
        <ul>
            <li>Budget: {{this.dataValues.budget}}</li>
            <li><a href="mailto:{{this.dataValues.email}}" class="btn btn-reverse">Apply Now</a></li>
        </ul>
        <div class="tech">
            <small>Technologies Needed: <span>{{this.dataValues.technologies}}</span></small>
        </div>
    </div>
    {{else}}
    <div class="gig">
        <p>No Gigs Available</p>
    </div>
    {{/each}}
</section>